{
  "name": "pinia-plugin-persistedstate",
  "version": "1.2.1",
  "description": "Configurable persistence and rehydration of Pinia stores.",
  "keywords": [
    "pinia-plugin"
  ],
  "repository": "https://github.com/prazdevs/pinia-plugin-persistedstate.git",
  "license": "MIT",
  "author": "Sacha 'PraZ' Bouillez",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "coverage": "vue-demi-switch 3 && vitest --run --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint --ext .ts",
    "lint:fix": "eslint --fix --ext .ts",
    "release": "bumpp  -t -p -c \":bookmark: release v\"",
    "size": "size-limit",
    "test:ui": "vue-demi-switch 3 && vitest --ui",
    "test:watch": "vue-demi-switch 3 && vitest --watch",
    "test:2": "vue-demi-switch 2 vue2 && vitest --run index",
    "test:3": "vue-demi-switch 3 && vitest --run",
    "test": "pnpm run test:2 && pnpm run test:3"
  },
  "peerDependencies": {
    "pinia": "^2.0.0"
  },
  "devDependencies": {
    "@prazdevs/eslint-config-typescript": "^3.0.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@vitest/ui": "^0.2.7",
    "@vue/composition-api": "^1.4.5",
    "bumpp": "^7.1.1",
    "c8": "^7.11.0",
    "eslint": "8.8.0",
    "happy-dom": "^2.31.1",
    "pinia": "^2.0.11",
    "size-limit": "^7.0.8",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vitepress": "^0.21.6",
    "vitest": "^0.2.7",
    "vue": "^3.2.29",
    "vue-demi": "^0.12.1",
    "vue2": "npm:vue@^2.6.14"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "1 kB"
    }
  ],
  "volta": {
    "node": "16.13.1"
  }
}
